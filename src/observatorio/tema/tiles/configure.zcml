    xmlns:i18n="http://namespaces.zope.org/i18n"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="observatorio.tema">

  <plone:tile
      name="observatorio.tema.daviz"
      title="Daviz Tile"
      description="Um tile que exibe um grÃ¡fico Daviz."
