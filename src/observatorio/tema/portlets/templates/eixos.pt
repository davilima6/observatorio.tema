<div id="eixos">
    <h5>Eixos de Atuação</h5>
    <ul>
        <tal:items repeat="item view/get_eixos">
        <li tal:attributes="id string:at-${item/id}">
            <a tal:attributes="title item/Title;
                               href item/absolute_url"
               tal:content="item/Title"></a>
        </li>
        </tal:items>
    </ul>
</div>
