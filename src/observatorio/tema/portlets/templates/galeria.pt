<div id="galeria-de-fotos" tal:define="items view/get_imagens">
    <h5 class="retranca">Galeria de fotos</h5>
    <ul class="small-block-grid-3">
        <li tal:repeat="item items">
            <a tal:attributes="href string:${item/parentURL}/view">
                <img tal:attributes="src string:${item/getURL|nothing}/image_galleria;
                                     alt item/Description;
                                     data-title item/Title|nothing;
                                     data-description item/Description|nothing;" /></a>
        </li>
    </ul>
</div>
